{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build:dev": "cross-env UMI_ENV=dev umi build",
    "build:test": "cross-env UMI_ENV=testing umi build",
    "build": "cross-env UMI_ENV=pro umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "test:inspect-brk": "node --inspect-brk ./node_modules/.bin/umi test",
    "analyze": "cross-env ANALYZE=1 umi build",
    "serve": "serve ./dist"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@ant-design/pro-form": "^1.7.4",
    "@ant-design/pro-layout": "^6.5.17",
    "@ant-design/pro-table": "^2.13.1",
    "@umijs/plugin-esbuild": "^1.0.1",
    "@umijs/preset-react": "^1.7.4",
    "antd": "^4.7.3",
    "classnames": "^2.2.6",
    "lint-staged": "^10.0.7",
    "moment": "^2.22.2",
    "omit.js": "^2.0.2",
    "prettier": "^1.19.1",
    "qs": "^6.7.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "umi": "^3.2.28",
    "umi-plugin-keep-alive": "0.0.1-beta.17",
    "umi-request": "^1.3.5",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "@types/mockjs": "^1.0.3",
    "@types/redux-logger": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "@umijs/preset-dumi": "1.1.0-rc.1",
    "@umijs/test": "^3.2.24",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^5.16.0",
    "eslint-config-umi": "^1.6.0",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "mockjs": "^1.1.0",
    "redux-logger": "^3.0.6",
    "typescript": "^4.0.5"
  }
}
